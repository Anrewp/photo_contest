<% pre_like = @photo.likes.find { |like| like.user_id == current_user.id  } if current_user %>
<div>

<%= link_to (image_tag('Thumbs_up.svg',size: "35x35")), photo_likes_path(@photo), remote:true, method: :post if !pre_like && current_user%>

<%= link_to (image_tag('Thumbs_up01.svg',size: "35x35")), photo_like_path(@photo, pre_like),remote:true, method: :delete if pre_like%>
	  	
</div>

<span><%= @photo.likes.count %></span>